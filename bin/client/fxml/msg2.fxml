<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<HBox alignment="TOP_RIGHT" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER_RIGHT" spacing="5.0" HBox.hgrow="ALWAYS">
         <children>
            <Label id="label" text="Label" textOverrun="CLIP">
               <font>
                   <Font name="GulimChe" size="13.0" />
               </font>
               <padding>
                  <Insets right="5.0" />
               </padding></Label>
            <HBox alignment="CENTER_RIGHT" spacing="5.0" VBox.vgrow="ALWAYS">
               <children>
                  <StackPane>
                     <children>
                        <StackPane opacity="0.25" style="-fx-background-color: #bfff00; -fx-background-radius: 10;">
                           <StackPane.margin>
                              <Insets />
                           </StackPane.margin></StackPane>
                        <TextFlow lineSpacing="2.0" maxHeight="-Infinity" textAlignment="RIGHT" StackPane.alignment="CENTER_RIGHT">
                           <children>
                              <Text id="text" fontSmoothingType="LCD" strokeType="OUTSIDE" strokeWidth="0.0" text="kkkkkkkkkkkkkk">
                                 <font>
                                      <Font name="GulimChe" size="13.0" />
                                 </font>
                                 <cursor>
                                    <Cursor fx:constant="TEXT" />
                                 </cursor>
                              </Text>
                           </children>
                           <StackPane.margin>
                              <Insets left="5.0" right="5.0" />
                           </StackPane.margin>
                        </TextFlow>
                     </children>
                  </StackPane>
                  <Circle id="circle" fill="WHITE" radius="15.0" stroke="WHITE" strokeType="INSIDE" strokeWidth="0.0" HBox.hgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets right="5.0" />
               </padding>
            </HBox>
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <padding>
            <Insets right="5.0" />
         </padding>
      </VBox>
   </children>
   <padding>
      <Insets left="80.0" />
   </padding>
</HBox>
